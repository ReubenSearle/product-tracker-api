{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "preLaunchTask": "npm: compile",
      "program": "${workspaceFolder}/src/server.ts",
      "outFiles": [
        "${workspaceFolder}/lib/**/*.js"
      ],
      "runtimeVersion": "14.15.3"
    }
  ]
}